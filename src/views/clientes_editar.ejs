<!-- <10 paso > -->
<% include partiales/_header %>
    <!-- <10 paso > -->


    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5 offset-md-3 card">
                <div class="card-body">
                    <!-- <10 paso > -->
                    <form action="/update/<%= data.Id %>" method="POST">
                        <!-- <10 paso > -->
                        <div class="form-group">
                            <input type="text" name="Nombre" value="<%= data.Nombre %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="Apellido" value="<%= data.Apellido %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="Direccion" value="<%= data.Direccion %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="Telefono" value="<%= data.Telefono %>" class="form-control">
                        </div>
                        <!-- <11 paso > -->
                        <button type="submit" class="btn btn-primary">Actualizar Comprador</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- <10 paso > -->
    <% include partiales/_footer %>